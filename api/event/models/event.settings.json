{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Event",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "minLength": 1
    },
    "description": {
      "type": "text",
      "unique": false,
      "minLength": 1,
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "in_house",
        "open_for_all"
      ]
    },
    "date": {
      "type": "date"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "startTime": {
      "type": "string"
    },
    "endTime": {
      "type": "string"
    }
  }
}
